    "wav": "^1.0.2",
    "wavesurfer.js": "^7.3.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.4.2",
    "@electron-toolkit/eslint-config": "^1.0.1",
    "@electron-toolkit/eslint-config-prettier": "^1.0.1",
    "@vitejs/plugin-react": "^4.0.4",
    "electron": "^25.6.0",
    "electron-vite": "^1.0.27",
    "eslint": "^8.47.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.0.2",
